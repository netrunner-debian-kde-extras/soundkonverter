### main.cpp ###

signal( SIGQUIT, cleanup );
signal( SIGINT, cleanup );

namespace
{
  const char* description = I18N_NOOP("KDE Time tracker tool");

  void cleanup( int )
  {
    kDebug(5970) << i18n("Just caught a software interrupt.");
    kapp->exit();
  }
}

